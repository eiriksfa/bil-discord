[flake8]
select = ANN,B,B9,BLK,C,D,DAR,E,F,I,S,W
max-complexity = 10
max-line-length = 80
ignore = E203, E501, W503, ANN101, ANN102, D107, D301, D105, S311
application-import-names = bil_discord,tests
import-order-style = google
docstring-convention = google
per-file-ignores =
    tests/*:S101,ANN,D103
    noxfile.py:ANN
